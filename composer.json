{
    "name": "villermen/runescape-lookup-commons",
    "description": "A PHP library containing RuneScape lookup components.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "Villermen\\RuneScape\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "symfony/var-dumper": "^4.0"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1.3"
        }
    }
}
